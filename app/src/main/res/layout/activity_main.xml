<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pengisian Form"
        android:textSize="30dp"
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:id="@+id/tvjd"
        />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="masukan nama"
        android:ems="15"
        android:textAlignment="center"
        android:inputType="textPersonName"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/tvjd"
        android:layout_marginTop="15dp"
        android:id="@+id/etnm" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="masukan tempat lahir"
        android:ems="15"
        android:textAlignment="center"
        android:inputType="text"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/etnm"
        android:layout_marginTop="15dp"
        android:id="@+id/etal" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="masukan tanggal lahir"
        android:ems="15"
        android:textAlignment="center"
        android:inputType="date"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/etal"
        android:layout_marginTop="15dp"
        android:id="@+id/etem" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etem"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:onClick="kirimdata"
        android:text="Submit" />




</RelativeLayout>